<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.MacPollo.lectorfacturas">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_i_icono_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LectorFacturas"
        android:usesCleartextTraffic="true">
        <activity android:name=".Actividades.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Actividades.FacturasRutaActivity" />
        <activity android:name=".Actividades.ScannerRutaActivity" />
        <activity android:name=".Actividades.ScannerActivity" />
        <activity android:name=".Actividades.MainActivity">

        </activity>
    </application>

</manifest>